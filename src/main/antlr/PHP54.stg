group PHP54;

file(namespaces) ::= <<
<namespaces; separator="\n">
>>

namespace(name, body) ::= <<
namespace<if(name)> <name><endif><body>
>>

body(statements) ::= <<
{<if(statements)>
	<statements; separator="\n">
<endif>}
>>

useDeclarationList(useDeclarations) ::= <<
use <useDeclarations; separator=",">;
>>

useDeclaration(type, alias) ::= <<
<type> as <alias>
>>
